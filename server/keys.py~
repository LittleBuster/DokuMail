__author__ = 'sergey'
class AppKeys():
    """
    Singleton class for loging app
    """

    def new(cls):
        if not hasattr(cls, 'instance'):
            cls.instance = super(Singleton, cls).__new__(cls)
            return cls.instance

    def get_config_key(self):
        key = {}
        key["key"] = b"1w1RTH2yaFIxk47D"
        key["IV"] = b"4srgJfVo"
        return key

    def get_cert_key(self):
        key = {}
        key["key"] = b'yI2lqMPxe0EpoV1o'
        key["IV"] = b'IPeRgElV'
        return key